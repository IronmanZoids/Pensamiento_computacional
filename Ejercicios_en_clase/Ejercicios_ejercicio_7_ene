Codigo de psint:
Ejercicio 1
  Algoritmo Calcula_la_Edad
    edad <- 0
    Escribir 'Ingresa tu edad '
    Leer edad
    Si edad>0 Y edad<120 Entonces
      Si edad>=18 Entonces
        Escribir 'Eres mayor de edad un adulto'
      SiNo
        Escribir 'Eres menor de edad'
      FinSi
    SiNo
      Escribir 'Edad invalida revisa tu edad'
    FinSi
  FinAlgoritmo

Ejercicio 2
Algoritmo Numero_par_non
	num<-0
	Escribir "Ingresa el numero"
	leer num
	
	Si num%2=0 Entonces
		Escribir "El numero ",num," es un numero par "
	SiNo
		Escribir "Es un numero impar ", num
	Fin Si
FinAlgoritmo


